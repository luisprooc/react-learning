){\r\n            case \"healt\":\r\n                saveHealt(--healt);\r\n                break;\r\n\r\n            case \"attack\":\r\n                saveAttack(--attack);\r\n                break;\r\n\r\n            case \"defense\":\r\n                saveDefense(--defense);\r\n                break;\r\n\r\n            default:\r\n                saveMagic(--magic);\r\n                break;\r\n\r\n        }\r\n        savePoints(++points);\r\n    };\r\n\r\n    const clean = () => {\r\n        saveHealt(0);\r\n        saveAttack(0);\r\n        saveDefense(0);\r\n        saveMagic(0);\r\n        savePoints(33);\r\n    };\r\n    \r\n    const reload = (e) => {\r\n        window.location.href=\"http://localhost:3000/\";\r\n    };\r\n    \r\n    \r\n    return(\r\n        <div className={Styles['rpg-card']}>\r\n            <h1>Clase Mago:</h1>\r\n            <h3>üßôüèº</h3>\r\n            <h5 className={!points?Styles['danger']:points <= 10?Styles[\"advert\"]:null}>Points: {points}</h5>\r\n            <center>\r\n                {mago.map((e,i) => {\r\n                    return(\r\n                        <figcaption key={i}>\r\n                            <p>{e[0]}: {e[1]}</p>\r\n                            <button name={e[0]} onClick={e =>{ add(e)}}>‚ûï</button>\r\n                            <button name={e[0]} onClick={e =>{ minus(e)}}>‚ûñ</button>\r\n                        </figcaption>\r\n                    );\r\n                })}\r\n            </center>\r\n            <div className={Styles['div-button']}>\r\n                <button onClick={e => clean()}>Clean</button>\r\n                <button onClick={e => reload(e)}>Default</button>\r\n            </div>\r\n            <center><a href=\"#\">Next ‚ñ∂</a></center>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default RpgCard;\r\n\r\n\r\n","C:\\Users\\LATITUDE\\Desktop\\Developer\\Cursos de udemy\\React\\Master React\\src\\components\\lazy\\image.js",[],{"ruleId":"60","replacedBy":"61"},{"ruleId